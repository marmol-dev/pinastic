{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "modules/core/images/icon-16.png",
    "128": "modules/core/images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/chromereload.js",
      "lib/async/lib/async.js",
      "lib/lodash/lodash.min.js",
      "lib/jquery/dist/jquery.min.js",
      "lib/backbone/backbone.js",
      "lib/backbone-deep-model/dist/backbone-deep-model.min.js",
      "background/backbone-localstorage.js",
      "background/api.js",
      "background/settings.js",
      "background/pin.js",
      "background/board.js",
      "background/contextual-menu.js",
      "background/notifications.js",
      "background/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "modules/popup/images/icon-19.png",
      "38": "modules/popup/images/icon-38.png"
    },
    "default_title": "pinastic",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "lib/jquery/dist/jquery.min.js",
        "content_scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "background",
    "contextMenus",
    "notifications",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
